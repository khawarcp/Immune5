{% schema %}
{
"name": "Footer",
"settings": [
{
"type" : "image_picker",
"id" : "image",
"label" : "Image"
},
{
"type" : "textarea",
"id" : "info",
"label" : "description"
},
{
"type" : "textarea",
"id" : "address",
"label" : "Address"
},
{
"type" : "text",
"id" : "phone",
"label" : "phone no"
},
{
"type" : "text",
"id" : "mail",
"label" : "Mail"
}

]
}
{% endschema %}

